{
    "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
    ],
    "parser": "babel-eslint",
    "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [
    1,
    {
    "extensions": [".js", ".jsx"]
    }
    ],
    "prettier/prettier": [
    "error",
    {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100
    }
    ]
    },
    "plugins": ["prettier"],
    "settings": {
        "import/resolver": {
          "alias": {
            "map": [
              ["components", "./src/components"],
            ],
            "extensions": [".ts", ".js", ".jsx", ".json"]
          }
        }
      }
    }